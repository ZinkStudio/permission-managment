<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!-- Soraya -->
		<navigation-case>
			<from-outcome>profileUpdate</from-outcome>
			<to-view-id>/profileUpdate.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>profileCreate</from-outcome>
			<to-view-id>/profileCreate.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>profileIndex</from-outcome>
			<to-view-id>/profileIndex.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<!-- louiza -->
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/userIndex.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>userIndex</from-outcome>
			<to-view-id>/userIndex.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userUpdate</from-outcome>
			<to-view-id>/userUpdate.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>userCreate</from-outcome>
			<to-view-id>/userCreate.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>userDelete</from-outcome>
			<to-view-id>/userIndex.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>userProfiles</from-outcome>
			<to-view-id>/userProfiles.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<!-- Antony -->
		<navigation-case>
			<from-outcome>permissionIndex</from-outcome>
			<to-view-id>/permissionIndex.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>permissionCreate</from-outcome>
			<to-view-id>/permissionCreate.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>permissionUpdate</from-outcome>
			<to-view-id>/permissionUpdate.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>permissionDelete</from-outcome>
			<to-view-id>/permissionIndex.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>permissionProfiles</from-outcome>
			<to-view-id>/permissionProfiles.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>languageIndex</from-outcome>
			<to-view-id>/languageIndex.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>languageCreate</from-outcome>
			<to-view-id>/languageCreate.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>languageUpdate</from-outcome>
			<to-view-id>/languageUpdate.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>languageDelete</from-outcome>
			<to-view-id>/languageIndex.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>labelIndex</from-outcome>
			<to-view-id>/labelIndex.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>labelCreate</from-outcome>
			<to-view-id>/labelCreate.xhtml</to-view-id>
			<redirect />
		</navigation-case>		
	</navigation-rule>
	
</faces-config>